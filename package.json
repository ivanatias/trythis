{
	"name": "@ivnatsr/trythis",
	"version": "1.0.0",
	"description": "Perform tasks without having to worry about using try/catch blocks. Inspired in Go's errors handling.",
	"author": "Ivan Atias <ivan.d.atias@gmail.com>",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test:dev": "vitest",
		"test": "vitest run",
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "pnpx @biomejs/biome lint",
		"format": "pnpx @biomejs/biome format --write",
		"check:": "pnpx @biomejs/biome check --write",
		"typecheck": "tsc --noEmit"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/ivanatias/trythis.git"
	},
	"homepage": "https://github.com/ivanatias/trythis#readme",
	"bugs": {
		"url": "https://github.com/ivanatias/trythis/issues"
	},
	"release": {
		"branches": ["main"]
	},
	"files": ["dist"],
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"keywords": ["try", "catch", "errors", "errors-handling", "TypeScript"],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"tsup": "8.2.4",
		"typescript": "5.5.4",
		"vitest": "2.0.5"
	}
}
